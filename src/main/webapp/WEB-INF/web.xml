<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>UserRegistrationServlet</servlet-name>
		<display-name>UserRegistrationServlet</display-name>
		<description></description>
		<servlet-class>servlet.UserRegistrationServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AdminLoginServlet</servlet-name>
		<display-name>AdminLoginServlet</display-name>
		<description></description>
		<servlet-class>servlet.AdminLoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ContactServlet</servlet-name>
		<display-name>ContactServlet</display-name>
		<description></description>
		<servlet-class>servlet.ContactServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddNewFlightServlet</servlet-name>
		<display-name>AddNewFlightServlet</display-name>
		<description></description>
		<servlet-class>servlet.AddNewFlightServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>UserRegistrationServlet</servlet-name>
		<url-pattern>/UserRegistrationServlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>AdminLoginServlet</servlet-name>
		<url-pattern>/AdminLoginServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ContactServlet</servlet-name>
		<url-pattern>/ContactServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddNewFlightServlet</servlet-name>
		<url-pattern>/AddNewFlightServlet</url-pattern>
	</servlet-mapping>
</web-app>
